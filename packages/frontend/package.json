  "scripts": {
    "dev": "VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` vite --port 4000 --host --clearScreen=false",
    "prebuild": "rm -rf build",
    "build": "tsc && VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` vite build",
    "build:custom": "tsc && vite build",
    "start": "VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` vite preview",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "check-types": "tsc --noEmit"
  },
