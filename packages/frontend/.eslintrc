{
  "env": {
    "node": false
  },
  "extends": [
    "../commons/.eslintrc",
    "react-app",
    "react-app/jest",
    "plugin:react/recommended"
  ],
  "ignorePatterns": [
    "public",
    "src/components/ProChart/charting_library",
    "analyze*"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    //   "@typescript-eslint/no-var-requires": "off"
    "@typescript-eslint/no-unused-vars": "off"
  }
}
