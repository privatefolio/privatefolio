{
  "name": "privatefolio-backend",
  "version": "2.0.0-alpha.0",
  "private": false,
  "license": "AGPL-3.0",
  "author": {
    "name": "Daniel Constantin",
    "email": "hello@danielconstantin.net"
  },
  "scripts": {
    "start": "APP_VERSION=`npm pkg get version` GIT_HASH=`git rev-parse HEAD` GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` bun src/start.ts",
    "prestart:watch": "KILL_PORT=4001 bun src/kill-process-on-port.ts",
    "start:watch": "APP_VERSION=`npm pkg get version` GIT_HASH=`git rev-parse HEAD` GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` bun --watch --no-clear-screen src/start.ts",
    "dev": "concurrently \"yarn build:watch\" \"yarn start:watch\" --kill-others --prefix \"{command} {time}\" --prefix-colors \"cyan,yellow\" --prefix-length 30 --timestampFormat \"[HH:mm:ss]\"",
    "ping": "bun src/ping.ts",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "prebuild": "rm -rf build",
    "build": "tsc --project tsconfig.lib.json && vite build",
    "build:watch": "tsc --project tsconfig.lib.json --watch --preserveWatchOutput",
    "check-types": "tsc --noEmit",
    "test": "BUN_SQL=false vitest",
    "test:bun": "BUN_SQL=true bun test test/bun/**",
    "test:ci": "GITHUB_CI=true BUN_SQL=false vitest run --watch false --reporter verbose && BUN_SQL=true yarn test:bun"
  },
  "dependencies": {
    "big.js": "^6.2.1",
    "croner": "^9.0.0",
    "ethers": "^6.9.0",
    "exponential-backoff": "^3.1.1",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "mime-types": "^2.1.35",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/bun": "^1.1.6",
    "@types/chalk": "^2.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chalk": "^5.3.0",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "16",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^6.2.2",
    "vitest": "^3.0.8"
  }
}
