{
  "name": "privatefolio",
  "version": "0.2.0",
  "private": false,
  "license": "AGPL-3.0",
  "author": {
    "name": "Daniel Constantin",
    "email": "hello@danielconstantin.net"
  },
  "scripts": {
    "dev": "VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` vite --port 4000 --host --clearScreen=false",
    "build": "tsc && VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)`  vite build",
    "start": "VITE_APP_VERSION=`npm pkg get version` VITE_GIT_HASH=`git rev-parse HEAD` VITE_GIT_DATE=`git show -s --format=%cI $(git rev-parse HEAD)` vite preview",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "db-server": "pouchdb-server -p 5984 -m",
    "gen:assets": "ts-node -O '{\"module\": \"commonjs\"}' scripts/generate-assets.ts",
    "gen:blockchains": "ts-node -O '{\"module\": \"commonjs\"}' scripts/generate-blockchains.ts",
    "gen:exchanges": "ts-node -O '{\"module\": \"commonjs\"}' scripts/generate-exchanges.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.157",
    "@mui/material": "^5.15.1",
    "@nanostores/logger": "^0.2.4",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@react-spring/web": "^9.7.3",
    "big.js": "^6.2.1",
    "comlink": "^4.4.1",
    "csv-parse": "^5.4.0",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "ethers": "^6.9.0",
    "events": "^3.3.0",
    "lightweight-charts": "^4.1.1",
    "lodash-es": "^4.17.21",
    "nanostores": "^0.9.5",
    "posthog-js": "^1.100.0",
    "pouchdb": "^8.0.1",
    "pouchdb-browser": "^8.0.1",
    "pouchdb-find": "^8.0.1",
    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.2.5",
    "@types/pouchdb": "^6.4.2",
    "@types/pouchdb-find": "^7.3.3",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "pouchdb-adapter-memory": "^8.0.1",
    "pouchdb-server": "^4.2.0",
    "prettier": "^2.5.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
